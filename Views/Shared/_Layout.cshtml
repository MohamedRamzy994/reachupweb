    <!-- -----------------------------------Start Navbar------------------------------ -->

    <div id="myhome">

        <nav class="navbar navbar-default navbar-fixed-top" role="navigation">

            <div class="container">

                <div class="navbar-header">

                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapsible" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                    <a class="navbar-brand logo" href="index.html">
                        <img src="~/Images/images/logo.png" class="img-responsive">
                    </a>

                </div>

                <div class="collapse navbar-collapse" id="navbar-collapsible">
                    @Html.Partial("_LoginPartial")
                    <ul class="nav navbar-nav navbar-right">

                        <li class="@ViewBag.currenthome"> <a href="@Url.Action("Index","Home")" data-value="myhome"> @Appletindex.Index.applettopmenuhome </a> </li>



                        <li class="@ViewBag.currentplans">
                            <a href="@Url.Action("Plans","Home")" data-value="Plans">
                                @Appletindex.Index.applettopmenuplans
                            </a>
                        </li>

                        <li class="@ViewBag.currentprojects"> <a href="@Url.Action("Projects","Home")" data-value="Projects">@Appletindex.Index.applettopmenuprojects </a> </li>

                        <li class="@ViewBag.currentteams"> <a href="@Url.Action("Teams","Home")" data-value="TEAM"> @Appletindex.Index.applettopmenuteams </a> </li>
                        <li class="@ViewBag.currentnews"> <a href="@Url.Action("News","Home")" data-value="myblog"> @Appletindex.Index.applettopmenublog</a> </li>
                        <li class="@ViewBag.currentabout"> <a href="@Url.Action("About","Home")" data-value="myblog"> @Appletindex.Index.applettopmenuabout </a> </li>
                        <li class="@ViewBag.currentcontact"> <a href="@Url.Action("Contact","Home")" data-value="myblog"> @Appletindex.Index.applettopmenucontact </a> </li>
                        <li>



                            @{ string lang = Thread.CurrentThread.CurrentCulture.Name;

                                if (lang == "ar-SA")
                                {

                                    lang = "en-US";
                                }
                                else if (lang == "en-US")
                                {
                                    lang = "ar-SA";
                                }



                                string Plan_Id = Url.RequestContext.HttpContext.GetOwinContext().Request.QueryString.HasValue && Request.QueryString["Plan_Id"] != null ? Request.QueryString["Plan_Id"].ToString() : "";
                                string News_Id = Url.RequestContext.HttpContext.GetOwinContext().Request.QueryString.HasValue && Request.QueryString["News_Id"] != null ? Request.QueryString["News_Id"].ToString() : "";



                            }


                            @if (!string.IsNullOrEmpty(Plan_Id))
                            {


                                <a href="@Url.Action(ViewContext.RouteData.Values["action"].ToString(), ViewContext.RouteData.Values["controller"].ToString(), new { Plan_Id=Plan_Id, language=lang})">
                                    @Appletindex.Index.appletchangelanguagebutton


                                    <i class="fa fa-language"></i>
                                </a>

                            }

                            else if (!string.IsNullOrEmpty(News_Id))
                            {
                                <a href="@Url.Action(ViewContext.RouteData.Values["action"].ToString(), ViewContext.RouteData.Values["controller"].ToString(), new { News_Id=News_Id, language=lang})">
                                    @Appletindex.Index.appletchangelanguagebutton


                                    <i class="fa fa-language"></i>
                                </a>
                            }
                            else
                            {

                                <a href="@Url.Action(ViewContext.RouteData.Values["action"].ToString(), ViewContext.RouteData.Values["controller"].ToString(), new {language=lang})">
                                    @Appletindex.Index.appletchangelanguagebutton


                                    <i class="fa fa-language"></i>
                                </a>
                            }
















                        </li>
                    </ul>

